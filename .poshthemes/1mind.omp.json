{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [

        {
          "type": "text",
          "style": "plain",
          "foreground": "#7b56ff",
          "properties": {
            "prefix":"",
            "text": "<#9494ff>┏</>",
            "postfix":""
          }
        },

        {
          "type": "exit",
          "style": "plain",
          "foreground": "#C94A16",
          "properties": {
            "prefix": "<#9494ff>[x]</>",
            "postfix":"",
            "display_exit_code": false
          }
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "#7b56ff",
          "properties": {
            "ssh_icon": "\uF817 ",
            "prefix": "<#9494ff>[</>",
            "display_user": false,
            "host_color": "#7b56ff",
            "postfix": "<#9494ff>: </>"
          }
        },

        {
          "type": "root",
          "style": "plain",
          "foreground": "#E06C75",
          "properties": {
            "root_icon": "root",
            "prefix": "",
            "postfix": " <#ffffff>::</> "
          }
        },

        {
          "type": "node",
          "style": "plain",
          "foreground": "#AAE682",
          "properties": {
            "display_version": false,
            "prefix": "\uE781",
            "postfix": " "
          }
        },

        {
          "type": "python",
          "style": "plain",
          "foreground": "#906cff",
          "properties": {
            "display_mode": "context",
            "display_version": false,
            "prefix": "\uE235",
            "postfix": " "
          }
        },

        {
          "type": "path",
          "style": "plain",
          "foreground": "#94bfff",
          "properties": {
            "style": "folder",
            "prefix": ""
          }
        },

        {
          "type": "git",
          "style": "plain",
          "foreground": "#C678DD",
          "properties": {
            "prefix": "",
            "display_status": true,
            "postfix":""
          }
        }

      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#9494ff",
          "properties": {
            "prefix": "┖",
            "text": "\u276F"
          }
        }
      ]
    }
  ]
}
